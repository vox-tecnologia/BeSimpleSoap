{
    "name": "vox-tecnologia/besimplesoap",
    "type": "library",
    "description": "Forked project BeSimple/Soap with added support for WSDL document/literal-wrapped, and more small things",
    "keywords": ["soap"],
    "homepage": "http://besim.pl",
    "license": "MIT",
    "authors": [
        {
            "name": "Francis Besset",
            "email": "francis.besset@gmail.com"
        },
        {
            "name": "Christian Kerl",
            "email": "christian-kerl@web.de"
        },
        {
            "name": "Andreas Schamberger",
            "email": "mail@andreass.net"
        }
    ],
    "require": {
        "php": ">=7.3",
        "ext-soap": "*",
        "ext-curl": "*",
        "ass/xmlsecurity": "~1.0",
        "symfony/framework-bundle": "~4.4",
        "symfony/twig-bundle": "~4.4",
        "zendframework/zend-mime": "2.1.*"
    },
    "require-dev": {
        "mikey179/vfsStream": "~1.0",
        "symfony/filesystem": "~4.4",
        "symfony/process": "~4.4"
    },
    "autoload": {
        "psr-0": { "BeSimple\\": "src/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.3-dev"
        }
    }
}
